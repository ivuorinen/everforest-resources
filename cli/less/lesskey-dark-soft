# Less termcap colors
export LESS_TERMCAP_mb=$'\e[1;1m'     # Begin blinking
export LESS_TERMCAP_md=$'\e[1;4m'    # Begin bold
export LESS_TERMCAP_me=$'\e[0m'                  # End mode
export LESS_TERMCAP_se=$'\e[0m'                  # End standout-mode
export LESS_TERMCAP_so=$'\e[3;0m'  # Begin standout-mode
export LESS_TERMCAP_ue=$'\e[0m'                  # End underline
export LESS_TERMCAP_us=$'\e[4;2m'   # Begin underline
